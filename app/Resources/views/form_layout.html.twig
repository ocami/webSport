{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('myDateTime/myDateTime.css') }}"/>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet"
          href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.0/css/bootstrap-datepicker.css">
{% endblock %}

{% block title %}
    {{ parent() }}
{% endblock %}

{% block body %}
    {% block forms_content %}{% endblock %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/validator.js') }}"></script>
    <script src="{{ asset('js/jqueryui.min.js') }}"></script>

    <! datepicker***************************************************************************************************>
    <script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.0/js/bootstrap-datepicker.min.js"></script>
    <script src="{{ asset('datepicker/js/datepicker-fr.js') }}"></script>
    <script src="{{ asset('myDateTime/myDateTime.js') }}"></script>
{% endblock %}
